# Configuración de Base de Datos PostgreSQL
# Copia este archivo a .env y completa los valores reales

PG_CONN=postgresql://username:password@hostname:port/seccionadora_logs

# Configuración de aplicación
ENVIRONMENT=local
LOG_LEVEL=INFO

